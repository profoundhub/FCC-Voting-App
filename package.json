{
  "name": "our-voting-app-fcc",
  "version": "1.0.1",
  "description": "Our Voting App. (FCC Project) | Pair Coding via c9",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/profoundhub/FCC-Voting-App.git"
  },
  "homepage": "https://our-voting-app.herokuapp.com/",
  "bugs": "https://github.com/profoundhub/FCC-Voting-App.git",
  "keywords": [
    "MongoDB",
    "Mongoose",
    "full stack",
    "Express",
    "Node",
    "javascript",
    "boilerplate"
  ],
  "scripts": {
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "test": "NODE_ENV=production mocha --compilers js:babel-core/register",
    "clean": "rimraf dist",
    "start": "npm run api:prod",
    "shrinkwrap": "npm shrinkwrap --dev",
    "start:try": "concurrently --kill-others \"npm run api:dev\" \"npm run server\"",
    "start:dev": "$(npm root)/.bin/webpack-dev-server --progress --inline --hot",
    "hot": "webpack-dev-server --progress --inline --hot",
    "app": "node",
    "server": "node server.js",
    "api:prod": "node api.js",
    "daniel": "node webDevServer.js",
    "api:dev": "set NODE_ENV=development&&node api.js",
    "postinstall": "webpack -p"
  },
  "author": "Daniel Lim and Sebastien Leon Flores",
  "license": "MIT",
